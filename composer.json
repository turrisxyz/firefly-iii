    "planning",
    "organizing",
    "bills",
    "personal finance",
    "budgets",
    "budgeting",
    "budgeting tool",
    "budgeting application",
    "transactions",
    "self hosted",
    "self-hosted",
    "transfers",
    "management"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/firefly-iii/firefly-iii",
  "type": "project",
  "readme": "readme.md",
  "authors": [
    {
      "name": "James Cole",
      "email": "james@firefly-iii.org",
